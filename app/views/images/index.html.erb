<h1 class="shiftss">Schedules</h1>

<table class="shifts">
  <thead>
    <tr>
      <th>Title</th>
      <th>Caption</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.title %></td>
        <td><%= image.caption %></td>
       <td><%= link_to 'Show', image %></td>
        <% if current_user.admin %>
       <td><%= link_to 'Edit', edit_image_path(image) %></td>
        <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
  
  
</table>
<button class="shifts"><%= link_to 'Dashboard', welcome_index_path %></button>




