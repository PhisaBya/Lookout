<p id="alert"><%= alert %></p>
<div class="container-fluid text-center">

   <div class="pic">
  
  </div>
<div class="bord">
<h1>Login</h1><br>
<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div><br>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div><br>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>
</div>